package practical1;

public class sum_square_odd {
  public static void main(String[] args) {
	  int sum = 0;
      int square = 0;

      for (int i = 33; i <= 55; i++) {
    	  if (i%2 != 0) {
         square = i * i ;
         sum = sum + square ;
      }
      }
      System.out.println("Sum of squares of odd numbers  from 33 to 55 = " + sum);
}
}
