package practical1;

public class product_square_even {
	public static void main(String[] args) {
		long product = 1;
	     for (int i = 3; i <= 12; i++) {
	   	  if (i%2 == 0) {
	       int square = i * i ;
	        product = product * square ;
	        
	     }
	     }
	     System.out.println("product of squares of even numbers  from 3 to 12 = " + product);
	}
}
