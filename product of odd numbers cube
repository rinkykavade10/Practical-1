package practical1;

public class product_cube_odd {
	public static void main(String[] args) {
        long product = 1;

        for (int i = 3; i <= 12; i++) {
        	if (i %2 !=0) {
            product = product * (i * i * i);
        }
        }
        System.out.println("Product of cubes of odd numbers  from 3 to 12 = " + product);
    }
}
